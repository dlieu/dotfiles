# other project related stuff go into each proj dir
# ln -sf ~/dotfiles/gitignore_user ~/.gitignore
# git config --global core.excludesfile ~/.gitignore

# ignore vim files
# Swap
[._]*.s[a-v][a-z]
!*.svg # comment out if you don't need vector files
[._]*.sw[a-p]
[._]s[a-rt-v][a-z]
[._]ss[a-gi-z]
[._]sw[a-p]

# Session
Session.vim
Sessionx.vim

# Temporary
.netrwhist
*~

# Auto-generated tag files
tags

# Persistent undo
[._]*.un~

# patch created .orig and .rej files
# [._]*.orig
# [._]*.rej
